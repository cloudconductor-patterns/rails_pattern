{
  "json_class": "Chef::Role",
  "default_attributes": {
    "backup_restore": {
      "sources": {
        "enabled": ["ruby", "directory"],
        "directory": {
          "paths": [
            "/var/www/shared/uploads"
          ]
        }
      },
      "destinations": {
        "enabled": ["s3"]
      }
    }
  },
  "run_list": [
    "recipe[rails_part::deploy]",
    "recipe[backup_restore::configure]"
  ]
}
